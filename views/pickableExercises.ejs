    <h1>Choose your Split for today</h1>

    <form action="/Datenbank" method="get">

        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Biceps" id="Biceps" name="Biceps">
            <label class="form-check-label" for="Biceps">
                Biceps
            </label>
        </div>


        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Triceps" id="Triceps" name="Triceps">
            <label class="form-check-label" for="Triceps">
                Triceps
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Legs" id="Legs" name="Legs">
            <label class="form-check-label" for="Legs">
                Legs
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Chest" id="Chest" name="Chest">
            <label class="form-check-label" for="Chest">
                Chest
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Abdominal" id="Abdominal" name="Abdominal">
            <label class="form-check-label" for="Abdominal">
                Abdominal
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Shoulders" id="Shoulders" name="Shoulders">
            <label class="form-check-label" for="Shoulders">
                Shoulders
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Lower Back" id="Lower Back" name="Lower Back">
            <label class="form-check-label" for="Lower Back">
                Lower Back
            </label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="Upper Back" id="Upper Back" name="Upper Back">
            <label class="form-check-label" for="Upper Back">
                Upper Back
            </label>
        </div>



        <div class="centering">
            <button type="submit" class="buttonGet">Continue</button>
        </div>
    </form>
    <hr class="rounded">
    <div class="centering">
        <button class="addExercise buttonAll centering">Add Exercise</button>
    </div>


